<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Workaround issues in MSBuild versions previous to 16.0. -->
  <PropertyGroup>
    <MSBuildAllProjects Condition=" '$(MSBuildVersion)' == '' Or '$(MSBuildVersion)' &lt; '16.0' ">
      $(MSBuildAllProjects);$(MSBuildThisFileFullPath)
    </MSBuildAllProjects>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Quantum.Development.Kit" Version="$(QdkVersion)" IsImplicitlyDefined="true" />
    <PackageReference Include="Microsoft.Quantum.Standard" Version="$(QdkVersion)" IsImplicitlyDefined="true" />
  </ItemGroup>

</Project>
